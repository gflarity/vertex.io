html(class='no-js', lang='en') 
  //<![endif]
  head!= partial('_head',{style_partial:'_stylesheets'})
  
  - if (app.constructor !== String)
    body.bp.account.apps
      #container
        header.apps: div.cinner!= partial('../account/_header')
        section#account(role='main'): div.cinner!= body
  - else
    body.bp.account.app
      #container
        header.app: div.cinner!= partial('../account/_header')
        section#account(role='main'): div.cinner!= body